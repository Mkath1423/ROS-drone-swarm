<launch>
	<arg name="world_file" default="trees.world" />

	<include file="$(find gazebo_ros)/launch/empty_world.launch">
		<arg name="world_name" value="$(find drone_swarm)/world/trees.world" />
		<arg name="paused" value="false" />
		<arg name="use_sim_time" value="true" />
		<arg name="gui" value="true" />
		<arg name="debug" value="false" />
	</include>
	
	<node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-file $(find drone_swarm)/urdf/drone.urdf -urdf -z 1 -model drone" />
	<node name="drone_controller" pkg="drone_swarm" type="drone_controller.py" args="model:=drone" output="screen"  ns="done1"/>
</launch> 
